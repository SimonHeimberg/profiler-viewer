{
    "name": "simon-heimberg/profiler-viewer",
    "description": "view profiler data of symfony from some other place",
    "type": "project",
    "keywords": ["profiler", "symfony"],
    "homepage": "https://github.com/SimonHeimberg/profiler-viewer",
    "license": "MIT",
    "authors": [
        {
            "name": "SimonHeimberg",
            "email": "simon.heimberg@heimberg-ea.ch"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "simon-heimberg/profiler-viewer-bundle": "0.*@beta",
        "symfony/console": "^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^4.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpstan/phpstan": "^0.12.19",
        "symfony/maker-bundle": "^1.15"
    },
    "support": {
        "issues": "https://github.com/SimonHeimberg/profiler-viewer/issues/"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "AppPV\\": "src/",
            "": "external_src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppPV\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear --no-warmup": "symfony-cmd",
            "assets:install --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
